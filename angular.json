    "data-store": {
      "projectType": "library",
      "root": "projects/data-store",
      "sourceRoot": "projects/data-store/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/data-store/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/data-store/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/data-store/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/data-store/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
